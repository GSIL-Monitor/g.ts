{
  "compilerOptions": {
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom",
      "es2016.array.include"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../../dist/tools/gulp",
    "strictNullChecks": true,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "sourceMap": true,
    "target": "es5",
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
    }
  },
  "files": [
    "gulpfile.ts"
  ]
}
